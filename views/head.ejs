<script>
// https://gist.github.com/nmsdvid/8807205
function debounceSearch (callback, time = 175) {
  let interval;
  return (event) => {
    clearTimeout(interval);
    interval = setTimeout(() => {
      interval = null;
      callback(event);
    }, time);
  };
};
</script>